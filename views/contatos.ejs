<h5 class="center">Formul√°rio de Cadastro</h5>
<div class="row">
    <form id="form" class="col s12">
        <div class="row">
            <div class="input-field col s12">
                <i class="material-icons prefix">account_circle</i>
                <input name='nome' id="nome" type="text" class="validate">
                <label for="icon_prefix">First Name</label>
            </div>
            <div class="input-field col s12">
                <i class="material-icons prefix">phone</i>
                <input name='telefone' id="telefone" type="tel" class="validate">
                <label for="icon_telephone">Telephone</label>
            </div>
        </div>
    </form>
    <button id="enviar" class="btn waves-effect waves-light" type="submit" name="action">Submit
        <i class="material-icons right">send</i>
    </button>
</div>

<script src="./js/jquery-3.2.1.min.js"></script>
<script src="./js/jquery-form.js"></script>
<script>
    $('#enviar').click(function () {
        $('#form').ajaxForm({
            url: '/salvar_usuario',
            type: 'post',
            success: function (data) {
                console.log('Dados salvos: ', data.result.nome)
            }
        }).submit();
    })

</script>